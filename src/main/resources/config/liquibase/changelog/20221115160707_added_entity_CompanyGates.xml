<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <!--
        Added the entity CompanyGates.
    -->
    <changeSet id="20221115160707-1" author="fabiodmota">
        <createTable tableName="t_company_gates">
            <column name="id" type="bigint"  autoIncrement="true" incrementBy="1" >
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="gate_name" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="company_group" type="varchar(255)">
                <constraints nullable="false" unique="true" uniqueConstraintName="ux_t_company_gates__company_group" />
            </column>
            <column name="company_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <!-- fabiodmota-needle-liquibase-add-column - fabiodmota will add columns here -->
        </createTable>
    </changeSet>


</databaseChangeLog>
